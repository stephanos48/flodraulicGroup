                <td>@Model.sTidwellEff</td>
                <td>@Model.rBrownEff</td>
                <td>@Model.aBizonEff</td>
                <td>@Model.kSwindlehurstEff</td>
                <td>@Model.pKimberlinEff</td>
                <td>@Model.dHerrEff</td>
                <td>@Model.dZielkeEff</td>
